[include]
src/

[ignore]
.*/node_modules/react-addons-test-utils/.*
.*/node_modules/fbjs/.*
.*/node_modules/filehound/.*
.*/node_modules/file-js/.*
.*/build|dist/.*

[libs]
flow-typed/

[options]
module.name_mapper='^\(.*\)\.styl$' -> '<PROJECT_ROOT>/src/interfaces/CSSFlowStub.js.flow'
module.name_mapper='^flow\/\(.*\)$' -> '<PROJECT_ROOT>/src/flow/\1'
module.name_mapper='^utils\/\(.*\)$' -> '<PROJECT_ROOT>/src/utils/\1'
module.name_mapper='^components\/\(.*\)$' -> '<PROJECT_ROOT>/src/components/\1'
module.name_mapper='^containers\/\(.*\)$' -> '<PROJECT_ROOT>/src/containers/\1'
module.name_mapper='^routes$' -> '<PROJECT_ROOT>/src/routes'
module.name_mapper='^store$' -> '<PROJECT_ROOT>/src/redux/store'
module.file_ext=.js
module.file_ext=.json
module.file_ext=.styl
